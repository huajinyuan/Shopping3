<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.zy.base.shopping">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:name=".mian.SApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mian.MainActivity" />
        <activity android:name=".mian.product.ProductsActivity" />
        <activity android:name=".mian.product.ProductInfoActivity" />
        <activity android:name=".mian.design.PublicDesignsActivity" />
        <activity android:name=".mian.design.DesignInfoActivity" />
        <activity android:name=".mian.center.EditInfoActivity" />
        <activity android:name=".mian.center.MyPaymentActivity" />
        <activity android:name=".mian.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mian.login.LoginActivity" />
        <activity android:name=".mian.login.RegisterActivity" />
        <activity android:name=".mian.login.RegisterNextActivity"></activity>
    </application>

</manifest>